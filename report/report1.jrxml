	<queryString>
		<![CDATA[SELECT
     fidele.`codeFidele` AS code,
     personne.`nom` AS  nom,
     personne.`prenom` AS prenom,
     SUM(contributionfidele.`montant`) AS montant,
     contributionfidele.`typecontribution` AS type
FROM
     `fidele` fidele INNER JOIN `contributionfidele` contributionfidele ON fidele.`idfidele` = contributionfidele.`fidele_idfidele`
     INNER JOIN `personne` personne ON fidele.`personne_idpersonne` = personne.`idpersonne`
GROUP BY
     montant, code]]>
	</queryString>
