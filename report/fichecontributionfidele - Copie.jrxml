	<queryString>
		<![CDATA[SELECT
     personne.`nom` AS personne_nom,
     personne.`prenom` AS personne_prenom,
     personne.`datenaiss` AS personne_datenaiss,
     personne.`lieunaiss` AS personne_lieunaiss,
     personne.`sexe` AS personne_sexe,
     personne.`email` AS personne_email,
     personne.`profession` AS personne_profession,
     personne.`telephone` AS personne_telephone,
     personne.`photo` AS personne_photo,
     personne.`statut_pro` AS statut_pro,
     fidele.`codeFidele` AS fidele_codeFidele,
     fidele.`statut` AS fidele_statut,
     fidele.`idfidele` AS fidele,
     zone.`nomzone` AS zone_nomzone,
     saintescene.`mois` AS saintescene_mois,
     saintescene.`annee` AS saintescene_annee,
     contributionfidele.`fidele_idfidele` AS contributionfidele_fidele_idfidele



FROM
     `personne` personne INNER JOIN `fidele` fidele ON personne.`idpersonne` = fidele.`personne_idpersonne`
     INNER JOIN `zone` zone ON personne.`zone_idzone` = zone.`idzone`
     INNER JOIN `contributionfidele` contributionfidele ON fidele.`idfidele` = contributionfidele.`fidele_idfidele`
     INNER JOIN `saintescene` saintescene ON contributionfidele.`saintescene_idsaintescene` = saintescene.`idsaintescene`
AND personne.lisible = 1
AND fidele.lisible = 1
AND contributionfidele.lisible = 1
AND saintescene.lisible = 1
AND fidele.idfidele = $P{param}
AND saintescene.idsaintescene = $P{param2}]]>
	</queryString>
